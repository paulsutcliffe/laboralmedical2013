= encabezado('Staff', '/assets/encabezado.jpg', '/assets/icono-alianzas.png', true)


#region-staff-show.container
  .region
    .region-inner.picture
      = image_tag @miembro.foto_url(:full), class: 'img-responsive'

      %h3.title
        = @miembro.nombre
        %br
        = @miembro.apellido
      %h4.subtitle
        = @miembro.puesto

    .region-inner.description
      = paragraph(raw(@miembro.descripcion))

