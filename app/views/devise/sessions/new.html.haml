= encabezado('Iniciar Sesion', '/assets/encabezado.jpg', '/assets/icono-cms.png', false)

#region-devise.container
  = form_for(resource, :as => resource_name, :url => session_path(resource_name)) do |f|
    %div
      = f.label :email
      %br/
      = f.email_field :email, :autofocus => true
    %div
      = f.label :password, 'Contrase√±a' 
      %br/
      = f.password_field :password
    - if devise_mapping.rememberable?
      %div
        = f.check_box :remember_me
        = f.label :remember_me, 'No volver a preguntar'
    %div= f.submit "Ingresar"
